<!DOCTYPE html>

<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Mike's Photoalbum</title>
        <!-- injector:css -->
        <!-- endinjector -->
    </head>
    <body class="grad0">



        <header class="lighbg">
            <div class="leftheader">

                <div class="title">Mike's Photo Albums</div>

                <div class="controls">
                    <div class="resize-controls">
                        <!-- <div class="resize-header">Image size:</div> -->

                        <button class="resize-sizers resize-enlarge"
                                onclick="resize.enlarge();" ></button>
                        <button class="resize-sizers resize-reduce"
                                onclick="resize.reduce();"></button>
                    </div>
                </div>
                <div id="breadcrumbs" class="breadcrumbs" ></div>
            </div>


            <div class="rightheader">

                    <div class="burger" onclick="burger();">
                        <div class="patty"></div>
                        <div class="patty"></div>
                        <div class="patty"></div>
                    </div>

                    <div id="burgerDropdown" class="dropdown hide">
                        <div id="myDropdown" class="dropdown-content">
                            <a href="#home">Home</a>
                            <a href="#about">About</a>
                            <a href="#contact">Contact</a>
                        </div>
                    </div>


            </div>

        </header>

        <p/>
        <div class="summary"></div>

        <div class="photos"></div>

        <div id="loading">Loading...</div>

        <!-- injector:js -->
        <!-- endinjector -->

    </body>
</html>
